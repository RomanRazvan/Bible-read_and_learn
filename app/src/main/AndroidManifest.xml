<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bible_readandlearn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BibleReadAndLearn">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".cartiActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".genezaActivity"
            android:parentActivityName=".cartiActivity"/>
        <activity android:name=".geneza_1Activity"
            android:parentActivityName=".genezaActivity"/>
        <activity android:name=".QuizQuestionActivity"
            android:parentActivityName=".cartiActivity"/>
        <activity android:name=".RezultatActivity"
            android:parentActivityName=".QuizQuestionActivity"/>
        <activity android:name=".informatiiActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".profetisiregicap2Activity"
            android:parentActivityName=".informatiiActivity"/>
        <activity android:name=".viataluiisusnastereActivity" />
    </application>

</manifest>